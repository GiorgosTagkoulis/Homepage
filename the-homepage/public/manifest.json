{
  "manifest_version": 2,
  "version": "0.0.1",
  "short_name": "React App",
  "name": "Create React App Sample",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "browser_action": {
    "default-popup": "popup.html"
  },
  "permissions": [
    "storage",
    "activeTab",
    "identity",
    "identity.email",
    "geolocation",
    "https://*/*"
  ]
}